
{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "jsx": "Preserve",
    "target": "es5",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowImportingTsExtensions": true
  },
  "paths": {
      "@/*": [
        "src/*"
      ]
  },
  "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
  ],
  "vueCompilerOptions": {
    "target": 3.4
  }
}
